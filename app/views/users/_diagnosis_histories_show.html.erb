<div id="result-r-<%= sizeindex + 1 %>" class="col-12 col-md-10 col-lg-6 round name-search-commentator">
	<div class="commentator-info" id="commentator-info-<%= sizeindex + 1 %>">
		<div class="commentator-name round">
			<%= diagnosis.commentator.name %>
		</div>
		<%= link_to image_tag(diagnosis.commentator.youtube_icon, class: 'youtube-icon'), diagnosis.commentator.youtube_channel_url %>
    <div class="video">
    <iframe width="110%" height="180" src="<%= diagnosis.commentator.popular_movies.pluck(:movie).first %>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe> 
    </div>
    <button id="show-button-<%= sizeindex + 1 %>" type="button" class="detail-button-2 show-button-2 round" value="<%= sizeindex + 1 %>"><i class="fa fa-angle-down"></i></button>
		<button id="close-button-<%= sizeindex + 1 %>" type="button" class="detail-button-2 close-button-2 round" value="<%= sizeindex + 1 %>" style="display: none;"><i class="fa fa-angle-up"></i></button>
    <div id="show-screen-<%= sizeindex + 1 %>" class="show-screen-2 round" style="display: none;">
			<p class="title round">特徴</p>
			<p>ゲームジャンルの割合</p>
			<%= pie_chart aggregate(diagnosis.commentator), height: '40%', width: '100%' %>
      <table class="show-question">
        <tbody>
          <tr>
            <td>プレイスタイル:</td>
            <td><%= diagnosis.commentator.play_style_i18n %></td>
          </tr>
          <tr>
            <td>顔出し:</td>
            <td><%= diagnosis.commentator.appearance_i18n %></td>
          </tr>
          <tr>
            <td>Vtuber:</td>
            <td><%= diagnosis.commentator.vtuber_i18n %></td>
          </tr>
          <tr>
            <td>性別:</td>
            <td><%= diagnosis.commentator.sex_i18n %></td>
          </tr>
          <tr>
            <td>グループ:</td>
            <td><%= diagnosis.commentator.is_forming_a_group_i18n %></td>
          </tr>
          <tr>
            <td>動画の長さ:</td>
            <td><%= diagnosis.commentator.movie_style.length_i18n %></td>
          </tr>
          <tr>
            <td>ライブの割合:</td>
            <td><%= diagnosis.commentator.movie_style.live_i18n %></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  <!-- <button type="button" class="btn btn-danger recommendation-button round similar-button-2" id="<%= index %>">似ている実況者は...</button> -->
</div>
  

